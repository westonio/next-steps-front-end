<h2>Search Results for <%= @category %></h2>

  <h3>Urgent Care results</h3>

  <div class="results-container">
    <% @results.urgent_care.each do |provider| %>
      <div class="result-item">
        <b>Provider name:</b> <%= provider.provider_name %><br>
        <b>Address:</b> <%= provider.street %><br>
        <b>City:</b> <%= provider.city %><br>
        <b>Description:</b> <%= provider.description[0, 100] %>...
      </div>
    <% end %>
  </div>
